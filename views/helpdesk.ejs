<script>
  
    $('.uploadImage').submit(function (e) {
      e.preventDefault();

      // var title = $('#title').val(); 
  
      $(this).ajaxSubmit({
        
        contentType: 'application/json',
        success: function (response) {
          console.log(response)
          if(response.status=='300' || response.status==300) alert(response.description)
          else {
         alert('Successfully..')
          window.location.reload()
          }
         
        }
      });
      return false;
    });
  
  
  
  </script>

