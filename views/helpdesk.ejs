<div class="block"><h1 class="title_block page-subheading">My Tickets</h1></div>
<div class="block-center" id="block-history">
<table class="table table-bordered table-striped table-labeled">
	<thead>
		<tr>
			<th>Ticket No.</th>
			<th>Subject</th>
			<th>Posted</th>
			<th>Last Staff Response</th>
			<th>Department</th>
			<th>Priority</th>
			<th>Status</th>
		</tr>
	</thead>
	<tbody>
		<% for(i=0;i<result[6].length;i++) { %>

					<tr onclick="window.location.href='/view-helpdesk?id=<%=result[6][i].ticket_number%>'" style="cursor: pointer;">
		
		<td><%=result[6][i].ticket_number%></td>
		<td><%=result[6][i].ticket_subject%></td>
		<td><%=result[6][i].date%></td>
		<td><%=result[6][i].update_date%></td>
		<td><%=result[6][i].t_department_id%></td>
		<td><%=result[6][i].t_priority_id%></td>
		<td><%=result[6][i].status%></td>
	</tr>
	<% } %>
					</tbody>
</table>
</div>

<div style="clear:both;"></div>



<p class="required"> <sup>*</sup> Required field</p>

<div class="box">
<form action="/helpdesk/insert" method="post" id="add_ticket" name="helpdesk_form" enctype="multipart/form-data" class="std uploadImage">
<h3 class="page-subheading">Post a New Ticket</h3>
<div class="form-group">
<div class="required text">
	<label for="title">Title <sup>*</sup> </label>
	<input type="text" name="ticket_subject" id="ticket_subject" title="Title" class="form-control is_required validate" data-validate="isGenericName" />
</div>
						<div class="required form-group">
	<label for="t_department_id">Department <sup>*</sup> </label>
	<select name="t_department_id" id="t_department_id" title="Department " class="form-control" style="width: 500px !important;">
		<option value="">Select Department</option>
							<option value="Software">Software</option>
							<option value="Accounts">Accounts</option>
							<option value="Shipping">Shipping</option>
							<option value="Delivery">Delivery</option>
						</select>
</div>
							
			<div class="required form-group">
	<label for="priority">Priority <sup>*</sup> </label>
	<select name="t_priority_id" id="t_priority_id" title="Priority " class="form-control" style="width: 500px !important;">
		<option value="">Select Priority</option>
							<option value="Critical">Critical</option>
							<option value="Scheduled">Scheduled</option>
							<option value="Major">Major</option>
						</select>
</div>
						<div class="required form-group">
	<label for="content">Message <sup>*</sup> </label>
	<textarea name="content" id="content" title="Message" class="form-control" rows="3" cols="26"></textarea>
</div>
			<div class="form-group">
	<label for="content">Attachment</label>
	<input name="helpdesk_attachment" id="helpdesk_attachment" value="" class="form-control" type="file" />
</div>
<p class="inline-infos required">Supported Types: jpg,doc,pdf,jpeg,png</p>						
			<p class="submit2">


				<input type="submit" class="btn btn-primary button" value="Post Ticket"  id="submitAddress">
</p>

</div>

</form>
